<% if @current_user %>
	<%= render 'search' %>
	
	<img id="mypic" src="https://graph.facebook.com/<%= @current_user['id'] %>/picture?type=normal" alt="profile picture">
	<div id="Welcome"> Welcome, <strong><%= @current_user['name'] %></strong></div>
	<% unless @matches.blank? %>
		<table id="matches" cellspacing="0px" cellpadding="0px" >		
			<% [@matches.count, 5].min.times do |n| %>
			<!-- <div id = "prophile_box">-->
			<tr bgcolor="grey">
			<td rowspan="2"><%= picture_link(@matches[n][0],"80")%></td>												
				<% 3.times do |k| %>
					<td><%= picture_link(@matches[n][1][1][k],"40")%></td>
				<% end %>
			</tr>
			<tr>
				<% 3.times do |k| %>
					<td><%= picture_link(@matches[n][1][1][k+3],"40")%></td>
				<% end %>
			</tr>						
					<!--</div><br />-->
				<% end %>						
		</table>
	<% end %>
<% else %>
welcome to my app please sign in
<% end %>