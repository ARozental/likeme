<h1>Listing users</h1>
<% users = User.where(:active => true).all %>
<% everyone = User.count %>
<br />

<%= everyone %> users<br />
<%= users.count %> active users
<table>
  <tr>
    <th>Name</th>
  </tr>

<% users.each do |user| %>
  <tr>
    <td><%= user.name %></td>

  </tr>
<% end %>
</table>

<br />
